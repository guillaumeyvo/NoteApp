$('#dSummernote').summernote({
    height: 420,
    placeholder: 'write here...',
    onChange: test
});


function test()
{
	var attr = $("#noteTitle").attr("noteid");
	if(typeof attr !== typeof undefined && attr !== false){
		setTimeout(function () {
		$("#saveMessage").empty();
		$("#saveMessage").text("Enregistrement en cours ...");
		//console.log($('#dSummernote').code());
		autoSave();
	}, 1000);
	}
	
    
}

